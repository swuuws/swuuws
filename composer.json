{
    "name": "swuuws/swuuws",
    "type": "project",
    "description": "Swuuws is a lightweight and easy to use PHP framework",
    "keywords": [
        "framework",
        "swuuws"
    ],
    "license": "MIT",
    "homepage": "http://swuuws.com",
    "support": {
        "issues": "https://github.com/swuuws/swuuws/issues",
        "source": "https://github.com/swuuws/swuuws"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "swuuws/framework": ">=2.0"
    },
    "require-dev": {
        "swuuws/framework": ">=2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/controller/",
            "model\\": "app/model/",
            "extend\\": "app/extend/",
            "plugin\\": "app/plugin/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
